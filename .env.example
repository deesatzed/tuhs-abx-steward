OPENROUTER_API_KEY=sk-or-v1-c

OPENROUTER_BASE_URL=https://openrouter.ai/api/v1


# Real question (keep neutral & short)
TEST_PROMPT=In one sentence (<=25 words), explain what a Python context manager does.
TEST_PROMPT_FALLBACK=Name three everyday objects, comma-separated.

# Debug controls
OPENROUTER_TEST_TIMEOUT_S=45
OPENROUTER_RESULTS_JSON=openrouter_test_results.json
OPENROUTER_DEBUG=1                     # 0=off, 1=on
OPENROUTER_DEBUG_DUMPS_DIR=openrouter_dumps
OPENROUTER_DEBUG_SSE_LINES=20          # how many SSE lines to show

# Application Configuration
NODE_ENV=development
PORT=8080

# OpenRouter API Configuration
# Get your API key from: https://openrouter.ai/settings/keys

OPENROUTER_SITE_NAME=TUHS Antibiotic Steward

# Model Configuration (OpenRouter model IDs)
# See available models: https://openrouter.ai/models
DEFAULT_MODEL=google/gemini-2.5-flash-lite-preview-09-2025
FALLBACK_MODEL=x-ai/grok-4-fast
COMPLEX_MODEL=qwen/qwen3-max
ICU_MODEL=x-ai/grok-4-fast

# Database Configuration
# PostgreSQL with pgvector extension
DATABASE_URL=postgresql://user:password@localhost:5435/antibiotic_db

# LLM Parameters
MAX_TOKENS=2000
TEMPERATURE=0.1
STREAM_RESPONSES=true

# Knowledge Base Configuration
KNOWLEDGE_TABLE=antibiotic_guidelines
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSIONS=1536

# Hybrid Search Weights (must sum to 1.0)
HYBRID_SEARCH_WEIGHT_SEMANTIC=0.6
HYBRID_SEARCH_WEIGHT_KEYWORD=0.4

# Rate Limiting
RATE_LIMIT_PER_MINUTE=10
RATE_LIMIT_PER_HOUR=30

# Audit Logging
AUDIT_LOG_DIR=./logs
AUDIT_LOG_LEVEL=info

# Feature Flags
ENABLE_MODEL_ROUTING=true
ENABLE_CONFIDENCE_SCORING=true
ENABLE_TOOL_CALLS=true
